{% extends '@cssTests/visual-regression-layout.html.twig' %}
{% import '@pulsar/pulsar/v2/helpers/tabs.html.twig' as tabs %}
{% set template %}{% include '@tests/tabs/template.partial.twig' %}{% endset %}
{% block body %}
{%
    set nav = [
        {
          "id"    : "alpha",
          "label" : "Alpha",
          "src"   : template
        },
        {
          "id"    : "bravo",
          "label" : "Bravo",
          "src"   : template,
          "active": true
        }
    ]
%}
{{ tabs.list(nav) }}
{{ tabs.content(nav) }}
{% endblock %}
