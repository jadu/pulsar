    "chai": "latest",
    "datatables.net-dt": "^1.10.10",
    "datatables.net-responsive-dt": "^2.0.0",
    "grunt": "latest",
    "grunt-asciify": "latest",
    "grunt-autoprefixer": "latest",
    "grunt-browserify": "latest",
    "grunt-bump": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-email-builder": "^3.0.1",
    "grunt-exec": "latest",
    "grunt-inline-css": "^0.1.5",
    "grunt-leading-indent": "latest",
    "grunt-mailgun": "^1.0.1",
    "grunt-mocha": "latest",
    "grunt-notify": "latest",
    "grunt-phpunit": "^0.3.6",
    "grunt-real-favicon": "^0.1.3",
    "grunt-sass": "latest",
    "jquery": "1.x",
    "jsdom": "latest",
    "matchdep": "latest",
    "minifyify": "^6.3.0",
    "mocha": "latest",
    "mocha-phantomjs": "latest",
    "sinon": "latest",
    "sinon-chai": "latest",
    "time-grunt": "latest",
    "uglifyify": "^3.0.1"
