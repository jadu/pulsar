{
  "name": "jadu-pulsar",
  "version": "13.0.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jadu/pulsar.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "workspaces": [
    "docs"
  ],
  "scripts": {
    "coverage": "mochify --transform [ babelify ] --plugin [ mochify-istanbul --exclude '**/+(tests|node_modules|libs)/**/*' --report lcov --dir ./coverage --instrumenter babel-istanbul] --reporter spec ./tests/harness/common './tests/js/web/**/*Test.js'",
    "start": "docker-compose up -d && grunt",
    "start:docs": "yarn workspace docs docusaurus start",
    "build:docs": "yarn workspace docs docusaurus build",
    "build:tests": "grunt javascript:tests",
    "build:tests:watch": "grunt javascript:tests:watch",
    "deploy:docs": "yarn workspace docs docusaurus deploy",
    "test": "npm run test:headless",
    "test:headless": "mochify --reporter spec --transform [ babelify ] ./tests/harness/common './tests/js/web/**/*Test.js'",
    "test:browser": "npm run build:tests && open ./tests/js/web/index.html",
    "test:browser:watch": "npm run build:tests:watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "chartist": "^1.0",
    "datatables.net": "^1.11.5",
    "datatables.net-buttons": "^2.0",
    "datatables.net-buttons-dt": "^2.0",
    "datatables.net-dt": "^1.11.5",
    "datatables.net-responsive": "^2.2.9",
    "datatables.net-responsive-dt": "^2.2.9",
    "datatables.net-select": "^1.3.4",
    "datatables.net-select-dt": "^1.3.4",
    "input-length": "^1.0.1",
    "jquery": "^3.6.0",
    "jquery-countdown": "^2.2.0",
    "jquery-placeholder": "^2.3.1",
    "jquery-sticky": "^1.0.4",
    "jquery-ui": "^1.13.1",
    "jstree": "^3.3.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "normalize.css": "^8.0.1",
    "pikaday": "^1.8.2",
    "pulsar-date-picker": "^1.0.0",
    "select2": "github:jadu/selectWoo#jadu-1.1.4",
    "spectrum-colorpicker": "^1.8.1",
    "timepicker": "^1.13.18",
    "tippy.js": "^6.0",
    "vide": "^0.5.1"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^10.0",
    "browser-sync": "^2.27.10",
    "browserify": "17.0",
    "browserify-istanbul": "^3.0",
    "browserify-shim": "^3.8.9",
    "chai": "^4.3.6",
    "chai-dom": "^1.11.0",
    "chai-hiff": "^1.0.0",
    "grunt": "^1.5.2",
    "grunt-asciify": "^0.2.2",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-bless": "^1.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-browserify": "^6.0",
    "grunt-bump": "^0.8",
    "grunt-casperjs": "^2.2.1",
    "grunt-contrib-clean": "^2.0",
    "grunt-contrib-compress": "^2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^3.0",
    "grunt-contrib-uglify": "^5.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^3.0",
    "grunt-leading-indent": "^0.2.0",
    "grunt-mocha": "^1.2.0",
    "grunt-notify": "^0.4.3",
    "grunt-real-favicon": "^0.2.4",
    "grunt-sass": "^3.1.0",
    "grunt-sass-replace": "^0.1.18",
    "grunt-scss-lint": "0.5",
    "grunt-text-replace": "^0.4.0",
    "jsdom": "^20.0",
    "jsdom-global": "^3.0",
    "matchdep": "^2.0",
    "minifyify": "^7.0",
    "mocha": "^10.0",
    "mochify": "^9.0",
    "mochify-istanbul": "^2.4.2",
    "require-globify": "^1.4.1",
    "sinon": "^14.0",
    "sinon-chai": "^3.0",
    "sinon-jquery": "^1.0.3",
    "uglifyify": "^5.0"
  }
}
