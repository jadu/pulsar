{
    "esversion": 6,
    "asi": true,
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "expr": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "maxlen": false,
    "maxparams": false,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true,
    "globals": {
        "$"          : true,
        "jQuery"     : true,
        "console"    : false,
        "define"     : false,
        "describe"   : false,
        "expect"     : false,
        "it"         : false,
        "before"     : false,
        "beforeEach" : false,
        "after"      : false,
        "afterEach"  : false,
        "sinon"      : false,
        "require"    : false,
        "pulsar"     : false
    }
}
