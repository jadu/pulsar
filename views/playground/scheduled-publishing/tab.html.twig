{% endblock tab_header %} #}

{% block tab_settings %}

<div id="accordionGroup" class="accordion">
    <div class="settings-section accordion__panel">
        <button type="button" aria-expanded="true" class="accordion__trigger" aria-controls="sect1" id="accordion1id">{{ html.icon('chevron-down', {'class': 'accordion__icon'}) }}</button>
        <h2 class="accordion__title" id="accordion1title">Properties</h2>
        <div id="sect1" role="region" aria-labelledby="accordion1title" class="accordion__content">
            <dl class="dl--horizontal u-margin-bottom-none">
                <dt>Status</dt>
                <dd>
                    {{ 
                        html.label({ 
                            'label': html.icon('check-circle') ~ ' Live',
                            'class': 'label--success'
                        })
                    }}
                    {{
                        html.link({
                            'label': 'View live',
                            'href': '#',
                            'class': 'u-float-right'
                        })
                    }}
                </dd>
                
                <dt>Page owner</dt>
                <dd>Paul Stanton</dd>
                
                <dt>Created</dt>
                <dd>4th July 2023 12:34</dd>
                
                <dt>Modified</dt>
                <dd>
                    5th July 2023 14:34
                </dd>
                <dd>
                    {{
                        html.link({
                            'label': 'Preview changes',
                            'href': '#'
                        })
                    }}
                </dd>
                
                <dt>Categories</dt>
                <dd>
                    <ul class="u-margin-bottom-none">
                        <li>Advice and Benefits</li>
                        <li>Environment</li>
                    </ul>
                </dd>
                <dd>
                    {{
                        html.link({
                            'label': 'Manage categories',
                            'href': '#'
                        })
                    }}
                </dd>
                
                <dt>Keywords</dt>
                <dd>Here, There, Everywhere</dd>
                
                <dt>Description</dt>
                <dd>There description will have a lot of text and we will likely need to truncate</dd>
                
                <dt>URL</dt>
                <dd>/advice-and-benefits/page-title</dd>
                <dd>
                    {{
                        html.link({
                            'label': 'Edit URL',
                            'href': '#'
                        })
                    }}
                </dd>
            </dl>
        </div>
    </div>

