{% block tab_settings %}

    <div class="settings-section">

    {{ html.icon('chevron-down', {'class': 'settings-section__toggle'}) }}
    {{ form.fieldset_start({ 'legend': 'Publishing' }) }}
    {{
        form.compound({
            'id': 'guid-' ~ random(),
            'class': 'form__group--flush',
            'inputs': [
                {
                    'class': 'form__control-col--6',
                    'label': 'Live status',
                    'id': random(),
                    'show-label': false,
                    'type': 'select2',
                    'data-html': 'true',
                    'data-search': 'false',
                    'options': [
                        {
                            'label': html.icon('circle-check', {'class': 'text--success'}) ~ ' Live',
                            'value': 'live'
                        },
                        {
                            'label': html.icon('square-xmark', {'class': 'text--danger'}) ~ ' Offline',
                            'value': 'offline'
                        }
                    ]
                },
                {
                    'class': 'form__control-col--6',
                    'label': 'Visibility',
                    'id': random(),
                    'show-label': false,
                    'type': 'select2',
                    'data-html': 'true',
                    'data-search': 'false',
                    'options': [
                        {
                            'label': html.icon('circle-check', {'class': 'text--success'}) ~ ' Visible',
                            'value': 'visible'
                        },
                        {
                            'label': html.icon('square-xmark', {'class': 'text--danger'}) ~ ' Invisible',
                            'value': 'invisible',
                            'selected': true
                        }
                    ]
                }
            ]
            
        })
    }}
    
        {{
            html.button({
                'label': 'Assign Categories'
            })
        }}

        {{
            html.button({
                'label': 'Assign Metadata'
            })
        }}

    {{ form.fieldset_end() }}
    </div>
